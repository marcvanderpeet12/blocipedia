<h2>Add collaborators to the wiki <strong><%= @wiki.title %></strong></h2>

<%= form_for ([@wiki, @collaboration]) do |f| %>
<% @users.each do |user| %>

    <p><%= check_box_tag 'user_ids[]', user.id %>
    <%= label_tag 'user_ids[]', user.email %> 
    <% end %> </p>
  
  <p> <%= f.submit %> <p>
<% end %>